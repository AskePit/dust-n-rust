                                    // Hit: 22x32
                                    (x: 0, y: 98, width: 22, height: 32),
                                    (x: 22, y: 98, width: 22, height: 32),

                                    // Idle: 20x32
                                    (x: 0, y: 130, width: 20, height: 32),
                                    (x: 20, y: 130, width: 20, height: 32),
                                    (x: 40, y: 130, width: 20, height: 32),
                                    (x: 60, y: 130, width: 20, height: 32),


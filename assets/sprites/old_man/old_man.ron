                                    (x: 60, y: 162, width: 30, height: 34),
                                    (x: 90, y: 162, width: 30, height: 34),
                                    (x: 120, y: 162, width: 30, height: 34),
                                ],
                            )),
                        ],
                        // optional
                        name: "old_man",
                    ),
                    // SpriteRenderPrefab
                    render: (
                        sheet: "old_man",
                        sprite_number: 0,
                    ),
                ),
                // AnimationSetPrefab
                animation_set: (
                    animations: [
                        (
                            // AnimationId
                            Walk,
                            (
                                samplers: [
                                    (
                                        0,
                                        // Only SpriteIndex channel allowed for SpriteRender in AnimationSetPrefab
                                        SpriteIndex,
                                        (
                                            // Time of key frames
                                            input: [0.0, 0.15, 0.35, 0.5, 0.65, 0.85, 1.0],
                                            // Sprite indexes from SpriteSheet for key frames
                                            output: [0, 1, 2, 3, 4, 5],
                                            // Sprites can only ever be animated with Step
                                            function: Step,
                                        ),
                                    ),
                                ],
                            ),
                        ),
                        (
                            // AnimationId
                            Attack,
                            (
                                samplers: [
                                    (
                                        0,

                                    (x: 0, y: 34, width: 25, height: 32),
                                    (x: 25, y: 34, width: 25, height: 32),
                                    (x: 50, y: 34, width: 25, height: 32),
                                    (x: 75, y: 34, width: 25, height: 32),

                                    // Death: 32x32
                                    (x: 0, y: 66, width: 32, height: 32),
                                    (x: 32, y: 66, width: 32, height: 32),
                                    (x: 64, y: 66, width: 32, height: 32),

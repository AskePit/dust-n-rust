#![enable(implicit_some)]
Prefab(
    entities: [
        // Old Man
        PrefabEntity(
            data: AnimationPrefab(
                // SpriteScenePrefab
                sprite_scene: (
                    // SpriteSheetPrefab with index 0
                    sheet: Sheet(
                        // TexturePrefab
                        texture: File("sprites/old_man/old_man.png", (
                            "IMAGE", (
                                sampler_info: (
                                    min_filter: Nearest,
                                    mag_filter: Nearest,
                                    mip_filter: Nearest,
                                    wrap_mode: (Tile, Tile, Tile),
                                    lod_bias: (0),
                                    lod_range: (
                                        start: ( 0 ),
                                        end: ( 8000 ),
                                    ),
                                    comparison: None,
                                    border: (0),
                                    anisotropic: Off,
                                    normalized: true,
                                ),
                            ),
                        )),
                        sprites: [
                            List((
                                texture_width: 150,
                                texture_height: 196,
                                sprites: [
                                    // Walk: 22x34
                                    (x: 0, y: 0, width: 22, height: 34),
                                    (x: 22, y: 0, width: 22, height: 34),
                                    (x: 44, y: 0, width: 22, height: 34),
                                    (x: 66, y: 0, width: 22, height: 34),
                                    (x: 88, y: 0, width: 22, height: 34),
                                    (x: 110, y: 0, width: 22, height: 34),

